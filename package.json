  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
